{
  "name": "iot-notebook",
  "version": "0.1.0",
  "description": "IoT Notebook Jupyter Lab extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://git.elite.polito.it/iot-notebook/iot-notebook.git",
  "bugs": {
    "url": "https://git.elite.polito.it/iot-notebook/iot-notebook.git/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Juan Pablo SÃ¡enz",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://git.elite.polito.it/iot-notebook/iot-notebook.git.git"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "install-ext": "jupyter labextension install . --no-build",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -b -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.2.6",
    "@jupyterlab/apputils": "^2.2.6",
    "@jupyterlab/coreutils": "^4.2.5",
    "@jupyterlab/launcher": "^2.2.5",
    "@jupyterlab/mainmenu": "^2.2.3",
    "@jupyterlab/notebook": "^2.2.6",
    "@jupyterlab/ui-components": "^2.2.4",
    "@lumino/coreutils": "1.5.3",
    "@lumino/signaling": "^1.4.3",
    "react": "16.9.0",
    "crypto": "1.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
